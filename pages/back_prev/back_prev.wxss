page{
  width: 100%;
  height: 100%;
}
.content{
  margin-bottom: 100rpx;
  width: 100%;
  height: auto;
}
.content .search{
  background-color: #f7f6fc;
  height: 48rpx;
  font-size: 14px;
  color: #999;
  width: 88.333%;
  padding-left: 10px;
  display:inline-block;
 
}
.radio-group{
  margin-bottom: 100rpx;
  width: 100%;
  height: auto;
}
checkbox{
  width: 24px;
  height: 24px;
  margin-right: 3px;
}
.text{
  font-size:12px;
  color:#999;
}
image{
  width: 32px;
  height: 32px;
  vertical-align:middle;
  margin-left:16px;
}
.nameZW{
  display: inline-block;
  vertical-align:middle;
  margin-left:16px;
}
.name{
  width: 700rpx;
}
.zimu{
  font-size:14px;
  color:#999;
}
label{
  /* height: 96rpx; */
  width: 100%;
  height: auto;
  display: inline-block;
}
button{
  width: 100%;
  background-color: #3399ff;
  color: #fff;
  border-radius: 0;
  height: 55px;
  line-height: 55px;
  font-size: 18px;
  position: fixed;
  bottom: 0;
  box-shadow: 8px 10px 10px  #ccc;
  border: 0px groove orange;
}
.btn-hover{
  box-shadow: 5px 6px 10px  #ccc;
  opacity: .7;
} 

